<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<modify version="11.0.1_9ac66cf38631f8473e37c39091c4b798f01bbf41">
    <add>
        <annotations>
            <annotation name="hygienic"/>
        </annotations>
        <type-metadata>
            <param name="type" value="module"/>
            <param name="fully-qualified-name" value="com.autoever.poc.test.TestCompare"/>
        </type-metadata>
        <memory-model-settings/>
        <dynamic-variables>
            <dynamic-variable initial-expression="tuple(string() as terminalID, string() as eventName, string() as eventValue)" name="compareParam" setter-expression="currentParam" setter-stream="ParamOutputStream"/>
        </dynamic-variables>
        <data name="QueryTable" shared="false" source="concrete" type="querytable">
            <param name="storage-method" value="heap"/>
            <param name="replicated" value="false"/>
            <table-schema>
                <schema>
                    <field name="terminalID" type="string"/>
                    <field name="eventName" type="string"/>
                    <field name="eventValue" type="string"/>
                </schema>
                <primary-index type="btree">
                    <field name="terminalID"/>
                </primary-index>
            </table-schema>
            <param name="truncate" value="false"/>
            <preload format="csv" mode="empty"/>
        </data>
        <stream name="InputStream">
            <schema>
                <field name="terminalID" type="string"/>
                <field name="data1" type="string"/>
                <field name="data2" type="string"/>
            </schema>
        </stream>
        <box name="Split" type="split">
            <input port="1" stream="InputStream"/>
            <output port="1" stream="out:Split_1"/>
            <output port="2" stream="out:Split_2"/>
            <output port="3" stream="out:Split_3"/>
            <param name="output-count" value="3"/>
        </box>
        <box name="Filter2" type="filter">
            <input port="1" stream="out:Split_3"/>
            <output port="1" stream="out:Filter2_1"/>
            <param name="autogen-nomatch-port" value="false"/>
            <param name="expression.0" value="isempty(compareParam.eventValue) || (compareParam.eventName == input1.data1 &amp;&amp; compareParam.eventValue != input1.data2)"/>
        </box>
        <box name="Filter3" type="filter">
            <input port="1" stream="out:Split_1"/>
            <output port="1" stream="out:Filter3_1"/>
            <param name="autogen-nomatch-port" value="false"/>
            <param name="expression.0" value="!isempty(compareParam.terminalID) &amp;&amp; !isempty(compareParam.eventName)"/>
        </box>
        <box name="Query" type="query">
            <input port="1" stream="out:Split_2"/>
            <output port="1" stream="out:Query_1"/>
            <dataref id="querytable" name="QueryTable"/>
            <param name="operation" value="read"/>
            <param name="where" value="primary-key"/>
            <param name="limit" value="1"/>
            <param name="key-field.0" value="terminalID"/>
            <param name="key-lowerbound-value.0" value="input.terminalID"/>
            <param name="key-lowerbound-operation.0" value="eq"/>
            <param name="key-upperbound-operation.0" value="none"/>
            <param name="write-type" value="insert"/>
            <param name="if-write-fails" value="ignore"/>
            <param name="no-match-mode" value="output-no-match-null"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="none"/>
                <item name="current" selection="all"/>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="Filter" type="filter">
            <input port="1" stream="out:Query_1"/>
            <output port="1" stream="out:Filter_1"/>
            <param name="autogen-nomatch-port" value="false"/>
            <param name="expression.0" value="!isempty(terminalID) &amp;&amp; !isempty(eventName)"/>
        </box>
        <box name="Query2" type="query">
            <input port="1" stream="out:Filter3_1"/>
            <output port="1" stream="out:Query2_1"/>
            <dataref id="querytable" name="QueryTable"/>
            <param name="operation" value="write"/>
            <param name="where" value="primary-key"/>
            <param name="key-field.0" value="terminalID"/>
            <param name="key-value.0" value="compareParam.terminalID"/>
            <param name="write-type" value="insert"/>
            <param name="if-write-fails" value="force"/>
            <param name="no-match-mode" value="output-no-match-null"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="all"/>
                <item name="old" selection="none"/>
                <item name="new" selection="none"/>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
                <expressions>
                    <set field="eventName">compareParam.eventName</set>
                    <set field="eventValue">compareParam.eventValue</set>
                </expressions>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="Split2" type="split">
            <input port="1" stream="out:Filter2_1"/>
            <output port="1" stream="out:Split2_1"/>
            <output port="2" stream="out:Split2_2"/>
            <param name="output-count" value="2"/>
        </box>
        <box name="Map" type="map">
            <input port="1" stream="out:Filter_1"/>
            <output port="1" stream="out:Map_1"/>
            <target-list>
                <item name="input" selection="none"/>
                <expressions>
                    <include field="currentParam">tuple(input1.terminalID as terminalID, input1.eventName as eventName, input1.eventValue as eventValue)</include>
                    <include field="status">'fromTable'</include>
                </expressions>
            </target-list>
        </box>
        <box name="Map2" type="map">
            <input port="1" stream="out:Split2_1"/>
            <output port="1" stream="OutputStream"/>
            <target-list>
                <item name="input" selection="all"/>
                <expressions>
                    <include field="compValue">compareParam</include>
                </expressions>
            </target-list>
        </box>
        <box name="Map3" type="map">
            <input port="1" stream="out:Split2_2"/>
            <output port="1" stream="out:Map3_1"/>
            <target-list>
                <item name="input" selection="none"/>
                <expressions>
                    <include field="currentParam">tuple(input1.terminalID as terminalID, input1.data1 as eventName, input1.data2 as eventValue)</include>
                    <include field="status">'updated'</include>
                </expressions>
            </target-list>
        </box>
        <output-stream name="OutputStream"/>
        <box name="Union" type="union">
            <input port="1" stream="out:Map_1"/>
            <input port="2" stream="out:Map3_1"/>
            <output port="1" stream="ParamOutputStream"/>
            <param name="strict" value="false"/>
        </box>
        <output-stream name="ParamOutputStream"/>
    </add>
</modify>
